<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@drawable/background"
	android:id="@+id/layout" android:paddingTop="230px"
	android:paddingLeft="80px" android:paddingRight="80px"
	android:paddingBottom="80px">
	<com.farsitel.apps.athantime.views.PanelSwitcher
		android:id="@+id/panelswitch" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_weight="4">
		<LinearLayout android:id="@+id/tabcontent"
			android:layout_width="fill_parent" android:layout_height="fill_parent">
			<LinearLayout android:layout_width="fill_parent"
				android:layout_height="fill_parent" android:orientation="vertical">
				<RelativeLayout android:layout_width="fill_parent"
					android:layout_height="wrap_content">
					<TextView android:gravity="center" android:padding="3dip"
						android:layout_centerHorizontal="true" android:textColor="#000"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:id="@+id/todayID" />
					<Button android:gravity="center"
						android:layout_alignParentRight="true" android:paddingRight="8dip"
						android:textColor="#000" android:background="@drawable/forward_left"
						android:layout_width="48px" android:layout_height="48px"
						android:id="@+id/nextBtn" android:layout_toLeftOf="@id/todayID" />
				</RelativeLayout>




				<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:id="@+id/athanLayout1" android:stretchColumns="*"
					android:paddingLeft="20px" android:paddingRight="20px">
					<!-- <TableRow> <TextView android:text="@string/latitude" android:padding="3dip" 
						android:textColor="#000" /> <TextView android:padding="3dip" android:id="@+id/latitudeID" 
						android:textColor="#000" /> </TableRow> <TableRow> <TextView android:text="@string/longitude" 
						android:padding="3dip" android:textColor="#000" /> <TextView android:padding="3dip" 
						android:textColor="#000" android:id="@+id/longitudeID" /> </TableRow> -->
					<TableRow>
						<TextView android:text="@string/fajr" android:padding="3dip"
							android:id="@+id/fajrStr" android:textColor="#000" />
						<TextView android:gravity="left" android:padding="3dip"
							android:textColor="#000" android:id="@+id/fajrID" />
					</TableRow>
					<TableRow>
						<TextView android:text="@string/sunrise" android:padding="3dip"
							android:id="@+id/sunriseStr" android:textColor="#000" />
						<TextView android:gravity="left" android:padding="3dip"
							android:textColor="#000" android:id="@+id/sunriseID" />
					</TableRow>
					<TableRow>
						<TextView android:text="@string/noon" android:padding="3dip"
							android:id="@+id/noonStr" android:textColor="#000" />
						<TextView android:gravity="left" android:padding="3dip"
							android:textColor="#000" android:id="@+id/noonID" />
					</TableRow>
					<!-- <TableRow> <TextView android:text="@string/asr" android:padding="3dip" 
						/> <TextView android:gravity="right" android:padding="3dip" android:id="@+id/eveningAthanID" 
						/> </TableRow> -->
					<TableRow>
						<TextView android:text="@string/night" android:padding="3dip"
							android:id="@+id/nightStr" android:textColor="#000" />
						<TextView android:gravity="left" android:padding="3dip"
							android:textColor="#000" android:id="@+id/nightID" />
					</TableRow>
					<TableRow>
						<TextView android:text="@string/nightAthan" android:id="@+id/nightAthanStr"
							android:textColor="#000" android:padding="3dip" />
						<TextView android:gravity="left" android:padding="3dip"
							android:textColor="#000" android:id="@+id/nightAthanID" />
					</TableRow>

					<!-- <TableRow> <TextView android:text="@string/midnight" android:padding="3dip" 
						/> <TextView android:gravity="right" android:padding="3dip" android:id="@+id/midnightID" 
						/> </TableRow> -->
				</TableLayout>


			</LinearLayout>
		</LinearLayout>

		<LinearLayout android:id="@+id/tabcontent2"
			android:layout_width="fill_parent" android:layout_height="fill_parent">

			<LinearLayout android:layout_width="fill_parent"
				android:layout_height="fill_parent" android:orientation="vertical">
				<RelativeLayout android:layout_width="fill_parent"
					android:layout_height="wrap_content">
					<TextView android:gravity="center" android:padding="3dip"
						android:layout_centerHorizontal="true" android:textColor="#000"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:id="@+id/tommorrowId" />
					<Button android:gravity="center"
						android:background="@drawable/backward_right"
						android:layout_alignParentLeft="true" android:paddingLeft="8dip"
						android:textColor="#000" android:layout_width="48px"
						android:layout_toRightOf="@id/tommorrowId" android:layout_height="48px"
						android:id="@+id/previous" />
				</RelativeLayout>

				<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:paddingLeft="20px" android:paddingRight="20px" android:id="@+id/athanLayout2"
					android:stretchColumns="*">

					<!-- <TableRow> <TextView android:text="@string/latitude" android:padding="3dip" 
						android:textColor="#000" /> <TextView android:padding="3dip" android:id="@+id/latitudeID2" 
						android:textColor="#000" /> </TableRow> <TableRow> <TextView android:text="@string/longitude" 
						android:padding="3dip" android:textColor="#000" /> <TextView android:padding="3dip" 
						android:textColor="#000" android:id="@+id/longitudeID2" /> </TableRow> -->

					<TableRow>

						<TextView android:text="@string/fajr" android:padding="3dip"
							android:id="@+id/fajrStr2" android:textColor="#000" />
						<TextView android:gravity="left" android:padding="3dip"
							android:textColor="#000" android:id="@+id/fajrID2" />

					</TableRow>
					<TableRow>

						<TextView android:text="@string/sunrise" android:padding="3dip"
							android:id="@+id/sunriseStr2" android:textColor="#000" />
						<TextView android:gravity="left" android:padding="3dip"
							android:textColor="#000" android:id="@+id/sunriseID2" />

					</TableRow>
					<TableRow>

						<TextView android:text="@string/noon" android:padding="3dip"
							android:id="@+id/noonStr2" android:textColor="#000" />
						<TextView android:gravity="left" android:padding="3dip"
							android:textColor="#000" android:id="@+id/noonID2" />

					</TableRow>
					<!-- <TableRow> <TextView android:text="@string/asr" android:padding="3dip" 
						/> <TextView android:gravity="right" android:padding="3dip" android:id="@+id/eveningAthanID" 
						/> </TableRow> -->
					<TableRow>

						<TextView android:text="@string/night" android:padding="3dip"
							android:id="@+id/nightStr2" android:textColor="#000" />
						<TextView android:gravity="left" android:padding="3dip"
							android:textColor="#000" android:id="@+id/nightID2" />

					</TableRow>
					<TableRow>
						<TextView android:text="@string/nightAthan" android:id="@+id/nightAthanStr2"
							android:textColor="#000" android:padding="3dip" />
						<TextView android:gravity="left" android:padding="3dip"
							android:textColor="#000" android:id="@+id/nightAthanID2" />

					</TableRow>
					<!-- <TableRow> <TextView android:text="@string/midnight" android:padding="3dip" 
						/> <TextView android:gravity="right" android:padding="3dip" android:id="@+id/midnightID" 
						/> </TableRow> -->
				</TableLayout>

			</LinearLayout>

		</LinearLayout>





	</com.farsitel.apps.athantime.views.PanelSwitcher>


</LinearLayout>
